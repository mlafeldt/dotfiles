alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

alias t='tree'
alias ta='tree -a'

alias ..='cd .. && pwd'
alias ...='cd ../.. && pwd'

command -v hub >/dev/null && alias git='hub'

alias bx='bundle exec'
alias bi='bundle install --jobs=3 --retry=3'
alias bu='bundle update --jobs=3 --retry=3'

alias vag='/usr/bin/vagrant'

alias gti=git

:
